     yoffs[y][mx]=-1
    end
   end
   n=1 -- find y match
   for my=y+1,h do
    if (grid[my][x]~=s) break
    n+=1
   end
   if n>=3 then -- y matched
    mtotal+=1
    for my=y,y+n-1 do
     yoffs[my][x]=-1
    end
   end
   ::match_end::
