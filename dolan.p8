  if collides(collmasks,px,py) then
   -- collision
   -- compute how far py can go
   -- before it hits this tile.
   local py0=cary+p[2]
   local pys=(py0|0x7)\1
   if abs(pys-py)>abs(yfix) then
    yfix=pys-py
   end
  end
 end
 if yfix~=0 then
  cary2+=yfix
  -- zero velocity
  carvy=0
 end
 cary=cary2
