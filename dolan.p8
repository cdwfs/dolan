 -- sky
 rectfill(0,0,127,8*(16-h),12)
 -- sun
 line(sunx,suny-12,sunx,suny+12,10)
 line(sunx-8,suny-8,sunx+8,suny+8,10)
 line(sunx-12,suny,sunx+12,suny,10)
 line(sunx-8,suny+8,sunx+8,suny-8,10)
