      s=rnd(sid_car_debris),
      px=pxs,
      py=py,
      vx=carvx,
      vy=-10+rnd(4),
     })
    end
   end
  end
 end
 if xfix~=0 then
  sfx(sfx_engine,-2)
  carx2+=xfix
