function isotrifill(x0,y0,x1,y2,c)
 local w,h=1+x1-x0,1+y2-y0
 local dx=w*0.5/h
 for y=y0,y2 do
  line(x0,y,x1,y,c)
  x0+=dx
  x1-=dx
 end
end

function drawtl()
 cls(0)
 -- pole left cap
 clip(0,8,8,8)
 ovalfill(0,10,15,21,7)
 clip(0,16,8,8)
